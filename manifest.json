{
    "manifest_version": 3,
    "name": "Home Tab",
    "version": "0.0.1",
    "chrome_url_overrides": {
        "newtab": "sketch/index.html"
    },
    "permissions": [
        "bookmarks",
        "tabs",
        "storage"
    ],
    "icons": {
        "16": "/sketch/img/icon-bitty.png",
        "48": "/sketch/img/icon-small.png",
        "128": "/sketch/img/icon-large.png"
    }
}